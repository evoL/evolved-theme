   "semanticHighlighting": true,
   "semanticTokenColors": {
      "*.annotation": "#c3827d",
      "annotation": "#c3827d",
      "property.annotation": "#c3827d"
   },
   "tokenColors": [
      {
         "name": "Comment",
         "scope": [
            "comment"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#aab6b2"
         }
      },
      {
         "name": "String",
         "scope": [
            "string",
            "markup.inline.raw.string",
            "markup.raw.inline.markdown"
         ],
         "settings": {
            "foreground": "#9fbf83"
         }
      },
      {
         "name": "String interpolation and other fancy stuff in literals",
         "scope": [
            "punctuation.definition.template-expression",
            "constant.character.escape",
            "string.interpolated.expression"
         ],
         "settings": {
            "foreground": "#84a16b"
         }
      },
      {
         "name": "Accented string quotes",
         "scope": [
            "punctuation.definition.string",
            "punctuation.definition.raw"
         ],
         "settings": {
            "foreground": "#84a16b"
         }
      },
      {
         "name": "Number",
         "scope": [
            "constant.numeric"
         ],
         "settings": {
            "foreground": "#36a0da"
         }
      },
      {
         "name": "Keywords",
         "scope": [
            "constant.language",
            "keyword.control",
            "keyword.declaration",
            "keyword.other",
            "storage.type",
            "storage.modifier",
            "keyword.operator.type.js",
            "support.function.svelte"
         ],
         "settings": {
            "foreground": "#d7a4a0"
         }
      },
      {
         "name": "this",
         "scope": [
            "variable.language.this",
            "variable.language.special.self",
            "variable.parameter.function.language.special.self"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#e75e98"
         }
      },
      {
         "name": "Operators",
         "scope": [
            "keyword.operator",
            "storage.modifier.optional.js"
         ],
         "settings": {
            "foreground": "#84a16b"
         }
      },
      {
         "name": "Types",
         "scope": [
            "support.class",
            "support.type.builtin",
            "support.type.primitive",
            "support.type.unknown",
            "entity.name.class",
            "entity.name.interface",
            "entity.name.label",
            "entity.name.type",
            "entity.other.inherited-class",
            "storage.modifier.array.js",
            "storage.type.protobuf",
            "storage.type.primitive.protobuf",
            "variable.type"
         ],
         "settings": {
            "foreground": "#c58823"
         }
      },
      {
         "name": "Variables",
         "scope": [
            "variable.other.constant",
            "variable.other.readwrite",
            "entity.name.variable"
         ],
         "settings": {
            "foreground": "#737873"
         }
      },
      {
         "name": "Functions",
         "scope": [
            "entity.name.function",
            "meta.function-call.python",
            "support.function",
            "variable.function"
         ],
         "settings": {
            "foreground": "#267fae"
         }
      },
      {
         "name": "Function parameters",
         "scope": [
            "variable.parameter"
         ],
         "settings": {
            "foreground": "#9e6a12"
         }
      },
      {
         "name": "Object properties",
         "scope": [
            "variable.object.property",
            "variable.other.property",
            "meta.attribute.python",
            "meta.mapping.key",
            "meta.property.object",
            "entity.name.tag.yaml",
            "entity.name.function.jsonnet",
            "punctuation.definition.variable.js",
            "meta.class variable.other.readwrite",
            "meta.interface variable.other.readwrite",
            "support.type.property-name.json",
            "meta.mapping.key string.unquoted",
            "meta.mapping.key string.quoted"
         ],
         "settings": {
            "foreground": "#006391"
         }
      },
      {
         "name": "Accented key-value operator",
         "scope": [
            "punctuation.separator.key-value",
            "punctuation.separator.dictionary.key-value",
            "keyword.operator.type.annotation.ts",
            "meta.mapping punctuation.separator.key-value.mapping",
            "meta.mapping.key string.quoted punctuation.definition.string"
         ],
         "settings": {
            "foreground": "#004e79"
         }
      },
      {
         "name": "Enums",
         "scope": [
            "entity.name.enum",
            "entity.name.type.enum"
         ],
         "settings": {
            "foreground": "#c58823"
         }
      },
      {
         "name": "Enum members",
         "scope": [
            "variable.other.enummember",
            "meta.enum variable.other.readwrite"
         ],
         "settings": {
            "foreground": "#805000"
         }
      },
      {
         "name": "Tags",
         "scope": [
            "entity.name.tag"
         ],
         "settings": {
            "foreground": "#267fae"
         }
      },
      {
         "name": "Tag beginning/end",
         "scope": [
            "punctuation.definition.tag"
         ],
         "settings": {
            "foreground": "#006391"
         }
      },
      {
         "name": "Attribute name",
         "scope": [
            "entity.other.attribute-name"
         ],
         "settings": {
            "foreground": "#84a16b"
         }
      },
      {
         "name": "Attribute =",
         "scope": [
            "punctuation.separator.key-value"
         ],
         "settings": {
            "foreground": "#668050"
         }
      },
      {
         "name": "Variables",
         "scope": [
            "variable.css",
            "variable.argument.css",
            "variable.other.custom-property.css"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#006391"
         }
      },
      {
         "name": "Property values",
         "scope": [
            "meta.property-value.css"
         ],
         "settings": {
            "foreground": "#909894"
         }
      },
      {
         "name": "Namespace",
         "scope": [
            "entity.other.attribute-name.namespace"
         ],
         "settings": {
            "foreground": "#668050"
         }
      },
      {
         "name": "Namespace separator (colon)",
         "scope": [
            "punctuation.separator.namespace"
         ],
         "settings": {
            "foreground": "#4d643a"
         }
      },
      {
         "name": "Heading",
         "scope": [
            "markup.heading",
            "entity.name.section.markdown"
         ],
         "settings": {
            "foreground": "#36a0da"
         }
      },
      {
         "name": "Accented heading marker",
         "scope": [
            "punctuation.definition.heading"
         ],
         "settings": {
            "foreground": "#267fae"
         }
      },
      {
         "name": "Bold",
         "scope": [
            "markup.bold"
         ],
         "settings": {
            "fontStyle": "bold"
         }
      },
      {
         "name": "Italic",
         "scope": [
            "markup.italic"
         ],
         "settings": {
            "fontStyle": "italic"
         }
      },
      {
         "name": "Link title",
         "scope": [
            "string.other.link.title",
            "string.other.link.description",
            "meta.image.inline.description",
            "meta.link.inline.description"
         ],
         "settings": {
            "foreground": "#e9a332"
         }
      },
      {
         "name": "Accented link title brackets",
         "scope": [
            "punctuation.definition.link.begin",
            "punctuation.definition.link.end",
            "punctuation.definition.link.title",
            "punctuation.definition.link.description"
         ],
         "settings": {
            "foreground": "#c58823"
         }
      },
      {
         "name": "Link URL",
         "scope": [
            "markup.underline.link"
         ],
         "settings": {
            "foreground": "#c58823"
         }
      },
      {
         "name": "Accented link URL brackets",
         "scope": [
            "punctuation.definition.metadata.markdown",
            "punctuation.definition.metadata.begin",
            "punctuation.definition.metadata.end"
         ],
         "settings": {
            "foreground": "#9e6a12"
         }
      },
      {
         "name": "Fenced code blocks",
         "scope": [
            "punctuation.definition.markdown",
            "punctuation.definition.raw.code-fence"
         ],
         "settings": {
            "foreground": "#84a16b"
         }
      },
      {
         "name": "Fenced code block language",
         "scope": [
            "fenced_code.block.language",
            "meta.code-fence.definition constant.other.language-name"
         ],
         "settings": {
            "foreground": "#668050"
         }
      },
      {
         "name": "Lists",
         "scope": [
            "punctuation.definition.list.begin",
            "punctuation.definition.list_item"
         ],
         "settings": {
            "foreground": "#d7a4a0"
         }
      },
      {
         "name": "Blocks",
         "scope": [
            "punctuation.definition.keyword",
            "punctuation.definition.block.begin.svelte",
            "punctuation.definition.block.end.svelte"
         ],
         "settings": {
            "foreground": "#c3827d"
         }
      }
   ],
   "type": "light"
}
